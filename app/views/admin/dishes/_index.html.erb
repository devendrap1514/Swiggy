<% context.instance_eval do
  column "Images" do |dish|
    ul do
      dish.dish_images.each do |img|
        li image_tag(img, size: "50x40")
      end
     end
  end
  column :id
  column :dish_name
  column :category do |dish|
    dish.category.category_name
  end

  column :"In Cart", as: :sortable do |dish|
    dish.restaurant_dishes.joins(:cart_items).count
  end
  actions
end %>
